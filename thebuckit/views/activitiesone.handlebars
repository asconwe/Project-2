<div class="category-box">
  <div style="display: none" id="user-id" data-userid="{{profile.id}}"></div>
  <h3 class="category">Activity Search Results</h3>
  {{#if allActivities}} {{#each activities}} {{#unless this.complete}}
  <div id="activity-{{this.id}}" class="activity open">
      <h3 class="activity-name">{{this.itemName}}</h3>
      <h4 class="activity-location">{{this.location}}</h4>
      <hr>
      <p class="activity-description">{{this.description}}</p>
      <h5>Tags:</h5>
      {{#each this.TagActivities}}
      <p class="activity-tags">{{this.Tag.tag}}</p>
      {{/each}}
      <br><button class="inpt bttn join-activity" data-id="{{this.id}}" {{#if this.joined}}disabled{{/if}}>Join{{#if this.joined}}ed!{{/if}}</button>
      <div class="activity-friends">
          {{this.participants}}
      </div>
  </div>
  {{/unless}} {{/each}} {{/if}} 

  {{#unless allActivities}} {{#each activities}} {{#unless this.complete}}
  <div id="activity-{{this.id}}" class="activity open">
      <h3 class="activity-name">{{this.Activity.itemName}}</h3>
      <h4 class="activity-location">{{this.Activity.location}}</h4>
      <p class="activity-description">{{this.Activity.description}}</p>
      <h5>Tags:</h5>
      {{#each this.TagActivities}}
      <p class="activity-tags">{{this.Tag.tag}}</p>
      {{/each}}
      <br><button class="inpt bttn join-activity" data-id="{{this.id}}" {{#if this.joined}}disabled{{/if}}>Join{{#if this.joined}}ed!{{/if}}</button>
      <div class="activity-friends">
          {{this.participants}}
      </div>
  </div>
  {{/unless}} {{/each}} {{/unless}}
</div>